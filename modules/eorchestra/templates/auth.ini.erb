[uwsgi]
enable-threads=True
uid=nginx
gid=www
plugins=python
workers=1
threads=4
master=False
vacuum=True
chmod-socket=666
backtrace-depth=10
module=app:app
# specific auth settings
socket=/tmp/eorchestra.sock
env=FRESTQ_SETTINGS=/home/eorchestra/election-orchestra/base_settings.py
# needed environment for verificatum
env=HOME=/home/eorchestra
env=PATH=/home/eorchestra/venv/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin
env=CLASSPATH=:/usr/local/share/java/verificatum.jar:/usr/local/share/java/jgmpmee.jar:/usr/local/share/java/jecn.jar
env=LIBRARY_PATH=/usr/local/lib:
env=LD_LIBRARY_PATH=/usr/local/lib: